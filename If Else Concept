#include <iostream>
using namespace std;

int main()
{
  int num1, num2, number;
  cout << "Enter two integers to be added: ";
  cin >> num1 >> num2;

  number = num1+num2;

  cout << number << endl;

  if (number > 10)
  {
    cout << "This number is bigger than 10. ";
  }

  else
  cout << "This number is not bigger than 10. ";

  return 0;
}
