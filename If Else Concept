#include <iostream>
using namespace std;

int main()
{
  float num1, num2, number;
  cout << "Enter two integers to be added: \n";
  cin >> num1 >> num2;

  number = num1+num2;

  cout << number << endl;

  if (number > 10)
  {
    cout << "This number is bigger than 10. ";
  }

  else if (number < 10)
  {
  cout << "This number is not bigger than 10. ";
  }
  
  else
  {
  cout << "This number is 10. ";
  }

  return 0;
}
