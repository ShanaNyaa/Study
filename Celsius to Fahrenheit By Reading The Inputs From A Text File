#include <iostream>
#include <fstream>
using namespace std;

int main()
{
  float celsius, fahrenheit;

  ifstream myfile;
  {
    myfile.open("Input.txt");
    myfile >> celsius;
  }

  fahrenheit = (celsius * 9 / 5) + 32;

  ofstream yourfile;
  {
    yourfile.open("Output.txt");
    yourfile << celsius << "C = ";
    yourfile << fahrenheit << "F";
  }

  myfile.close();
  yourfile.close();

  return 0;
}
